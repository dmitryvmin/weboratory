{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node ./scripts/env-check.js && rimraf ./build ./dist",
    "prestart": "npm run tsc",
    "check": "npm run format && npm run lint",
    "format": "prettier --write \"**/*.+(js|yml|yaml|json)\"",
    "lint": "eslint --ext \".js,.ts\"",
    "clean": "rimraf package-lock.json node_modules",
    "tsc": "tsc",
    "start": "node ./dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --inspect --respawn ./src/index.ts",
    "build": "webpack ./src/index.ts --mode production --bail --progress --profile --display-error-details"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
